<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <!-- games.ForEach list out each game && icon -->
    <h1>GAME PAGE</h1>
    <% games.forEach(game => { %>
        <a href="/games/<%= game._id %>"><%= game.title %></a>
        <form action="/games/<%= game._id %>?_method=DELETE" method="POST">
            <input type="submit" value="DELETE">
        </form>
        <a href="/games/<%= game._id %>/edit">Edit</a>
    <% }) %>
    <a href="/games">Games Page</a>
    <a href="/devs">Developer Page</a>
</body>
</html>